{
  "name": "front",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "prebuild": "npm run generate:api",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "generate:api": "openapi-ts --input ./openapi/swagger.v1.json --output ./src/lib/api --client @hey-api/client-fetch",
    "generate:api:watch": "openapi-ts --watch --input ./openapi/swagger.v1.json --output ./src/lib/api --client @hey-api/client-fetch"
  },
  "dependencies": {
    "@microsoft/signalr": "^10.0.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/dompurify": "^3.0.5",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^14.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.0",
    "github-markdown-css": "^5.8.1",
    "lucide-vue-next": "^0.553.0",
    "luxon": "^3.7.2",
    "marked": "^17.0.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.22",
    "vue-draggable-plus": "^0.6.0",
    "vue-router": "^4.6.3",
    "vue-sonner": "^2.0.9",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.87.5",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.19.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}